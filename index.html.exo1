<!DOCTYPE html>
<html lang="en">
	<head>
		<title>My First Angular directive</title>
	</head>
	<body ng-app="myApp">
		<xx-header></xx-header>
		<xx-body></xx-body>
		<xx-footer></xx-footer>
		<script	type="text/ng-template" id="tmp1/header.html">
		<h1>Header</h1>
		<hr>
		</script>
		<script	type="text/ng-template" id="tmp1/body.html">
		<h1>Body</h1>
		</script>
		<script	type="text/ng-template" id="tmp1/footer.html">
		<hr>
		<h1>Footer</h1>
		</script>
	
		<script src="bower_components/angular/angular.js"></script>
		<script src="app.js"></script>
	</body>
</html>
